{
	"title": "2017-2018 School Year",
	"styles": {
		"website": "styles/all.css",
		"ebook": "styles/all.css",
		"pdf": "styles/all.css",
		"mobi": "styles/all.css",
		"epub": "styles/all.css"
	},
	"plugins": ["scripts", "image-wrapper", "autocover", "katex", "theme-gestalt", "-theme-default", "styles-sass-fix"],
	"pluginsConfig": {
		"scripts": {
			"files": ["./scripts/local-script.js"]
		},
		"autocover": {
			"font": {
				"size": null,
				"family": "Impact",
				"color": "#FFF"
			},
			"size": {
				"w": 1800,
				"h": 2360
			},
			"background": {
				"color": "#09F"
			}
		    "theme-gestalt": {
        		"logo": "/assets/logo.png",		// path to the logo file to use in the sidebar
        		"favicon": "/assets/favicon.png",	// path to your favicon
        		"excludeDefaultStyles": true		// excludes the pre-compiled theme css in favor of your custom css
    }
		}
	}
}
